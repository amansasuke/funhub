@import url('https://fonts.googleapis.com/css2?family=Red+Hat+Display:wght@500;700;900&display=swap');

:root {
    --pale-blue: hsl(225, 100%, 94%);
    --bright-blue: hsl(245, 75%, 52%);
    --very-pale-blue: hsl(225, 100%, 98%);
    --desaturated-blue: hsl(224, 23%, 55%);
    --dark-blue: hsl(223, 47%, 23%);
}

.card1 {

    min-height: 400px;

    border-radius: 10px;
    background: white;
}

.card1 .card1-header1 {
    width: 100%;
    height: 156px;
    border-radius: 10px 10px 0px 0px;
}

.card1 .card1-header1 img {
    width: 100%;
    border-radius: 10px 10px 0px 0px;
}

.card1 .card1-body {
    width: 100%;
    height: auto;
    padding: 25px;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.card1 .card1-body .card1-title {
    width: 100%;
    font-size: 20px;
    font-weight: 900;
    color: var(--dark-blue);
    text-align: center;
    padding: 15px;
    box-sizing: border-box;
}

.card1 .card1-body .card1-text {
    width: 100%;
    color: var(--desaturated-blue);
    text-align: center;
    line-height: 25px;
    padding: 15px 0px;
    box-sizing: border-box;
}

.card1 .card1-body .card1-plan {
    display: flex;
    flex-direction: row;
    align-items: center;
    column-gap: 15px;
    background: var(--very-pale-blue);
    border-radius: 10px;
    padding: 15px;
    box-sizing: border-box;
}

.card1 .card1-body .card1-plan .card1-plan-img {
    flex-grow: 1;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.card1 .card1-body .card1-plan .card1-plan-text {
    flex-grow: 6;
    display: flex;
    flex-direction: column;
    row-gap: 4px;
}

.card1 .card1-body .card1-plan .card1-plan-text .card1-plan-title {
    color: var(--dark-blue);
    font-weight: 900;
    font-size: 14px;
}

.card1 .card1-body .card1-plan .card1-plan-text .card1-plan-price {
    color: var(--desaturated-blue);
    font-size: 14px;
}

.card1 .card1-body .card1-plan .card1-plan-link {
    flex-grow: 1;
}

.card1 .card1-body .card1-plan .card1-plan-link a {
    color: var(--bright-blue);
    font-weight: 700;
    font-size: 14px;
    cursor: pointer;

}

.card1 .card1-body .card1-plan .card1-plan-link a:hover {
    color: #766cf1;
    text-decoration: none;
    ;
}

.card1 .card1-body .card1-payment-button {
    padding: 25px 0px 15px;
    box-sizing: border-box;
}

.card1 .card1-body .card1-payment-button a {
    width: 150px;
    height: 50px;
    border: 0;
    padding: 20px;
    background: var(--bright-blue);
    color: white;
    font-weight: 700;
    border-radius: 10px;
    box-shadow: 0px 10px 20px 0px hsl(245deg 75% 52% / 44%);
    cursor: pointer;
}

.card1 .card1-body .card1-payment-button a:hover {
    background: #766cf1;
}

.card1 .card1-body .card1-cancel-button {
    padding: 15px 0px;
    box-sizing: border-box;
}

.card1 .card1-body .card1-cancel-button a {
    width: 100%;
    border: 0;
    background: none;
    color: var(--desaturated-blue);
    font-weight: 900;
    text-align: center;
    cursor: pointer;
}

.card1 .card1-body .card1-cancel-button a:hover {
    color: var(--dark-blue);
}
</style>

